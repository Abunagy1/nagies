{
    "name": "project",
    "version": "1.0.0",
    "private": true,
    "description": "This is a demonstration project for handling token based authentication using node.js, express, mongo, jwt.",
    "main": "app.js",
    "keywords": [
        "nodejs",
        "express",
        "mongo",
        "token-based-authentication",
        "jwt",
        "mongoose"
    ],
    "author": "Mohamed Nagy",
    "license": "MIT",
    "scripts": {
        "start": "DEBUG=project:* nodemon ./bin/www",
        "clean": "rimraf dist/",
        "copy": "copyfiles -f src/images/*.jpg dist/images",
        "prettier": "prettier --config .prettierrc.json 'src/**/*.js' --write",
        "lint": "eslint './**/*.js' --fix",
        "jasmine": "jasmine",
        "create-dev-db": "node node_modules/db-migrate/bin/db-migrate db:create api",
        "create-test-db": "node node_modules/db-migrate/bin/db-migrate db:create api_test",
        "create-dev-sql": "node node_modules/db-migrate/bin/db-migrate create api --sql-file",
        "dev-migrate": "node node_modules/db-migrate/bin/db-migrate up -e dev",
        "test-migrate": "node node_modules/db-migrate/bin/db-migrate up -e test",
        "reset-dev-db": "node node_modules/db-migrate/bin/db-migrate reset -e dev",
        "reset-test-db": "node node_modules/db-migrate/bin/db-migrate reset -e test",
        "test": "db-migrate --env test up && ENV=test jasmine || db-migrate --env test down",
        "serverstart": "npm run copy && node ./bin/www"
    },
    "dependencies": {
        "async": "^3.2.4",
        "bcrypt": "^5.0.1",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.20.0",
        "compression": "^1.7.4",
        "config": "^3.3.8",
        "cookie-parser": "~1.4.4",
        "cors": "^2.8.5",
        "db-migrate": "^0.11.13",
        "db-migrate-pg": "^1.2.2",
        "debug": "~2.6.9",
        "dotenv": "^16.0.2",
        "ejs": "^3.1.8",
        "express": "~4.16.1",
        "express-validator": "^6.14.2",
        "helmet": "^6.0.0",
        "http-errors": "~1.6.3",
        "joi": "^17.6.2",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.21",
        "luxon": "^3.0.3",
        "mongoose": "^6.6.0",
        "morgan": "~1.9.1",
        "pg": "^8.8.0",
        "pug": "^3.0.2",
        "stylus": "^0.59.0",
        "superagent": "^8.0.0"
    },
    "devDependencies": {
        "copyfiles": "^2.4.1",
        "eslint": "8.23.1",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "jasmine": "^4.4.0",
        "jasmine-spec-reporter": "^7.0.0",
        "nodemon": "^2.0.19",
        "prettier": "^2.7.1",
        "supertest": "^6.2.4"
    }
}
